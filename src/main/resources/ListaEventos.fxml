<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.cell.PropertyValueFactory?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" spacing="10" xmlns:fx="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.unieventos.controller.ListaEventosController">
    <padding>
        <Insets top="20" right="20" bottom="20" left="20"/>
    </padding>
    <TableView fx:id="eventosTable">
        <columns>
            <TableColumn fx:id="nombreColumn" text="Nombre" />
            <TableColumn fx:id="ciudadColumn" text="Ciudad" />
            <TableColumn fx:id="fechaColumn" text="Fecha" />
        </columns>
    </TableView>
    <Button text="Ver Detalles" onAction="#verDetalles"/>
</VBox>
